generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  first_name String
  last_name String
  email String @unique
  password String
  profile_picture String
  badges Json
  active Boolean
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
